{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "vercel:install": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "registry:build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "registry:build": {
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$", ".env*", "registry.json"],
      "outputs": ["public/r/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false,
      "persistent": false
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    }
  }
}
