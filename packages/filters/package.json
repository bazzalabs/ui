{
  "name": "@bazzaui/filters",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./integrations/tanstack-table": {
      "types": "./dist/integrations/tanstack-table/index.d.ts",
      "import": "./dist/integrations/tanstack-table/index.js",
      "require": "./dist/integrations/tanstack-table/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:watch": "vitest --watch --disable-console-intercept"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@bazzaui/typescript-config": "*",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "tsup": "^8.5.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  }
}
